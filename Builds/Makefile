ifndef config
  config=debug32
endif
export config

PROJECTS := linux_standalone_stereo linux_standalone_multi linux_lv2_stereo linux_lv2_multi linux_vst_stereo linux_vst_multi

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

linux_standalone_stereo: 
